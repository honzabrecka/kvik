{
  "name": "rapid",
  "version": "0.0.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "jan.brecka@gmail.com",
  "license": "MIT",
  "dependencies": {
    "@types/random-js": "^1.0.28",
    "random-js": "^1.0.8"
  },
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "jest": "^20.0.4",
    "ts-jest": "^20.0.6",
    "typescript": "2.3"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.spec\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/node_modules/**"
    ]
  }
}
